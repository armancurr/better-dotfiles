{
    "window.commandCenter": false,
    "workbench.layoutControl.enabled": false,
    "editor.mouseWheelZoom": true,
    "editor.cursorBlinking": "smooth",
    "editor.cursorSmoothCaretAnimation": "on",
    "code-runner.clearPreviousOutput": true,
    "editor.links": false,
    "editor.fontFamily": "'JetBrains Mono', 'SF Mono', Consolas",
    "editor.fontLigatures": true,
    "window.title": " ",
    "editor.hover.enabled": false,
    "liveServer.settings.donotShowInfoMsg": true,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "editor.fontSize": 16,
    "terminal.integrated.fontSize": 16,
    "terminal.integrated.fontFamily": "'JetBrains Mono', 'SF Mono', Consolas",
    "git.autofetch": true,
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "editor.lineNumbers": "off",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "workbench.startupEditor": "none",
    "editor.minimap.enabled": false,
    "workbench.activityBar.location": "bottom",
    "window.newWindowProfile": "Default",
    "editor.overtypeCursorStyle": "line",
    "terminal.integrated.accessibleViewPreserveCursorPosition": true,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "underline",
    "terminal.integrated.cursorStyleInactive": "none",
    "editor.cursorStyle": "underline",
    "files.autoSave": "afterDelay",
    "workbench.colorTheme": "Tokyo Night Dark Enhanced",
    "workbench.productIconTheme": "fluent-icons",
    "github.copilot.enable": {
        "*": false,
        "plaintext": false,
        "markdown": false,
        "scminput": false
    },
    "window.menuBarVisibility": "compact"
}
