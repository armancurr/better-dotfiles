{
    "editor.mouseWheelZoom": true,
    "editor.cursorBlinking": "smooth",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.links": false,
    "editor.fontFamily": "'GeistMono NF', 'JetBrains Mono', 'Lilex Nerd Font Mono', monospace",
    "editor.fontLigatures": true,
    "window.title": " ",
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "editor.fontSize": 18,
    "terminal.integrated.fontSize": 18,
    "terminal.integrated.fontFamily": "'GeistMono NF', 'JetBrains Mono', 'Lilex Nerd Font Mono', monospace",
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "git.autofetch": true,
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "editor.lineNumbers": "off",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "editor.minimap.enabled": false,
    "workbench.activityBar.location": "bottom",
    "window.newWindowProfile": "Default",
    "editor.overtypeCursorStyle": "line",
    "terminal.integrated.accessibleViewPreserveCursorPosition": true,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "underline",
    "terminal.integrated.cursorStyleInactive": "none",
    "editor.cursorStyle": "underline",
    "workbench.productIconTheme": "fluent-icons",
    "chat.editing.alwaysSaveWithGeneratedChanges": true,
    "editor.lightbulb.enabled": "off",
    "window.zoomLevel": 0.5,
    "diffEditor.hideUnchangedRegions.enabled": true,
    "git.openRepositoryInParentFolders": "always",
    "workbench.tree.renderIndentGuides": "always",
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false,
    "editor.hover.enabled": false,
    "editor.parameterHints.enabled": false,
    "terminal.integrated.allowedLinkSchemes": [
        "file",
        "http",
        "https",
        "mailto",
        "vscode",
        "vscode-insiders",
        "ms-settings"
    ],
    "editor.unicodeHighlight.invisibleCharacters": false,
    "window.commandCenter": false,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "explorer.confirmPasteNative": false,
    "cursor.cpp.disabledLanguages": [
      "markdown",
      "scminput"
    ],
    "workbench.layoutControl.enabled": false,
    "files.autoSave": "afterDelay",
    "workbench.iconTheme": "symbols",
    "editor.wordWrap": "on",
    "workbench.colorTheme": "Vercel (Dark)",
    "workbench.panel.showLabels": false,
    "window.menuBarVisibility": "compact",
    "diffEditor.maxComputationTime": 0
  }
