test -f ~/.profile && . ~/.profile
test -f ~/.bashrc && . ~/.bashrc

PS1='\w > '

# NPM Aliases
alias ni='npm install'
alias nu='npm uninstall'
alias ns='npm start'
alias nr='npm run dev'
alias nb='npm run build'

# Git Aliases
alias ga='git add .'
alias gc='git commit -m'
alias gp='git push'
alias gs='git status'
alias gco='git checkout'
alias gb='git branch'
alias gpl='git pull'

# System Aliases
alias cls='clear'
alias .='cd ..'
alias ..='cd ../..'

#BUN Aliases
alias bn='bun init'
alias bi='bun install'
alias ba='bun add'
alias bu='bun uninstall'
alias bd='bun dev'
alias bs='bun start'
alias br='bun run build'

if [[ -f ~/.bashrc ]]; then
  source ~/.bashrc
fi
