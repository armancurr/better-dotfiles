{
    "window.commandCenter": false,
    "workbench.layoutControl.enabled": false,
    "editor.mouseWheelZoom": true,
    "editor.cursorBlinking": "smooth",
    "editor.cursorSmoothCaretAnimation": "on",
    "code-runner.clearPreviousOutput": true,
    "editor.links": false,
    "editor.fontFamily": "'Geist Mono', 'JetBrains Mono'",
    "editor.fontLigatures": true,
    "window.title": " ",
    "editor.hover.enabled": false,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "editor.fontSize": 16,
    "terminal.integrated.fontSize": 16,
    "terminal.integrated.fontFamily": "'Geist Mono', 'JetBrains Mono'",
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "git.autofetch": true,
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "editor.lineNumbers": "off",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "workbench.startupEditor": "none",
    "editor.minimap.enabled": false,
    "workbench.activityBar.location": "bottom",
    "window.newWindowProfile": "Default",
    "editor.overtypeCursorStyle": "line",
    "terminal.integrated.accessibleViewPreserveCursorPosition": true,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "underline",
    "terminal.integrated.cursorStyleInactive": "none",
    "editor.cursorStyle": "underline",
    "workbench.productIconTheme": "fluent-icons",
    "files.autoSave": "afterDelay",
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": false,
        "scminput": false
    },
    "chat.editing.alwaysSaveWithGeneratedChanges": true,
    "redhat.telemetry.enabled": true,
    "editor.lightbulb.enabled": "off",
    "workbench.editor.enablePreview": true,
    "window.menuBarVisibility": "compact",
    "workbench.iconTheme": "symbols",
    "window.zoomLevel": 1,
    "workbench.colorTheme": "Vercel (Dark)",
    "diffEditor.hideUnchangedRegions.enabled": true
}
