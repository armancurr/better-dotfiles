{
    "editor.mouseWheelZoom": true,
    "editor.cursorBlinking": "smooth",
    "editor.cursorSmoothCaretAnimation": "on",
    "code-runner.clearPreviousOutput": true,
    "editor.links": false,
    "editor.fontFamily": "'Lilex Nerd Font Mono', 'GeistMono NF', 'JetBrainsMono NF'",
    "editor.fontLigatures": true,
    "window.title": " ",
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "editor.fontSize": 16,
    "terminal.integrated.fontSize": 16,
    "terminal.integrated.fontFamily": "'Lilex Nerd Font Mono', 'GeistMono NF', 'JetBrainsMono NF'",
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "git.autofetch": true,
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "editor.lineNumbers": "on",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "editor.minimap.enabled": false,
    "workbench.activityBar.location": "bottom",
    "window.newWindowProfile": "Default",
    "editor.overtypeCursorStyle": "line",
    "terminal.integrated.accessibleViewPreserveCursorPosition": true,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "underline",
    "terminal.integrated.cursorStyleInactive": "none",
    "editor.cursorStyle": "underline",
    "workbench.productIconTheme": "fluent-icons",
    "files.autoSave": "afterDelay",
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": false,
        "scminput": false
    },
    "chat.editing.alwaysSaveWithGeneratedChanges": true,
    "redhat.telemetry.enabled": true,
    "editor.lightbulb.enabled": "off",
    "workbench.editor.enablePreview": true,
    "workbench.iconTheme": "symbols",
    "window.zoomLevel": 1,
    "diffEditor.hideUnchangedRegions.enabled": true,
    "git.openRepositoryInParentFolders": "always",
    "workbench.tree.renderIndentGuides": "always",
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false,
    "editor.hover.enabled": false,
    "editor.parameterHints.enabled": false,
    "terminal.integrated.allowedLinkSchemes": [
        "file",
        "http",
        "https",
        "mailto",
        "vscode",
        "vscode-insiders",
        "ms-settings"
    ],
    "github.copilot.selectedCompletionModel": "gpt-4o-copilot",
    "editor.unicodeHighlight.invisibleCharacters": false,
    "window.commandCenter": false,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "explorer.confirmPasteNative": false,
    "cursor.cpp.disabledLanguages": [
        "plaintext",
        "markdown",
        "scminput"
    ],
    "workbench.layoutControl.enabled": false,
    "window.menuBarVisibility": "compact",
    "workbench.colorTheme": "Vercel (Dark)"
}
